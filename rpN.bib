journal = {Clin. Neurophysiol.},
keywords = {EEG,Magnetic field,Nonlinear analysis,Phase space,Recurrence plot},
pages = {1195--1201},
title = {{Effect of low-frequency magnetic fields on brain electrical activity in human subjects}},

volume = {115},
year = {2004}
}
