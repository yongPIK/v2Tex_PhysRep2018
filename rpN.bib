title = {{Vibrations of a delivery car excited by railway track crossing}},

volume = {42},
year = {2009}
}
@article{urbanowicz2007,
abstract = {We measure the content of random uncorrelated noise in heart rate variability using a general method of noise level estimation using a coarse-grained entropy. We show that usually, except for atrial fibrillation, the level of such noise is within 5$\backslash$u201315{\%} of the variance of the data and that the variability due to the linearly correlated processes is dominant in all cases analyzed but atrial fibrillation. The nonlinear deterministic content of heart rate variability remains significant and may not be ignored.},
