keywords = {complex networks,dynamical transitions,palaeo-climate,recurrence plot},
title = {{Complex network approach for recurrence analysis of time series}},
url = {10.1016/j.physleta.2009.09.042}
}
@article{bigdeli2009,
abstract = {Market data analysis in Iran's electricity market as a market with a pay-as-bid payment mechanism has been considered in this paper. The analysis procedure includes both predictability and correlation analysis of the most important load and price indices. The experimental data from Iran's electricity market has been employed in its real size which is long enough to take properties such as non-stationarity of the market into account. For predictability, the characteristics of the hourly accepted {\textless}i{\textgreater}Weighted Average Price{\textless}/i{\textgreater} ({\textless}i{\textgreater}WAP){\textless}/i{\textgreater} as the topmost price index of this market is analyzed. The analysis tools are time series analysis methods such as power spectral density analysis, phase space reconstruction and test of surrogates, the fractional dimension and the slope of integral sums and the recurrence plots. The results indicate a deterministic, un-stationary and seasonal behavior in addition to unstable periodic orbits and even chaotic behavior in {\textless}i{\textgreater}WAP{\textless}/i{\textgreater} time series. These observations imply just short-term predictability of {\textless}i{\textgreater}WAP{\textless}/i{\textgreater} behavior. The interactive behavior of {\textless}i{\textgreater}WAP{\textless}/i{\textgreater} with the hourly {\textless}i{\textgreater}required load (RL){\textless}/i{\textgreater} is also considered. For this interaction analysis, in addition to the common correlation methods, cross and joint recurrence plot are also employed. The joint behavioral analysis represents an un-stationary mimic correlation between {\textless}i{\textgreater}WAP{\textless}/i{\textgreater} and {\textless}i{\textgreater}RL{\textless}/i{\textgreater}.},
annote = {Application},
author = {Bigdeli, N and Afshar, K},
journal = {Physica A},
keywords = {Chaos,Deregulation,Electricity market,Pay as bid,Price forecasting,Recurrence plots,Recurrence quantification analysis (RQA),Time series analysis},
pages = {1577--1592},
title = {{Characterization of Iran electricity market indices with pay-as-bid payment mechanism}},

volume = {388},
year = {2009}
}
@article{vlahogianni2007,
abstract = {The performance of signalized arterials is related to queuing phenomena. The paper investigates the effect of transitional traffic flow conditions imposed by the formation and dissipation of queues. A cross-recurrence quantification analysis combined with Bayesian augmented networks are implemented to reveal the prevailing statistical characteristics of the short-term traffic flow patterns under the effect of transitional queue conditions. Results indicate that transitions between free-flow conditions, critical queue conditions that exceed the detector$\backslash$u2019s length, as well as the occurrence of spillovers impose a set of prevailing traffic flow patterns with different statistical characteristics with respect to determinism, nonlinearity, non-stationarity and laminarity. The complexity in critical queue conditions is further investigated by introducing two supplementary regions in the critical area before spillover occurrence. Results indicate that the supplementary information on the transitional conditions in the critical area increases the accuracy of the predictive relations between the statistical characteristics of traffic flow evolution and the occurrence of transitions.},
annote = {Application},
author = {Vlahogianni, E I and {Webber Jr.}, C L and Geroliminis, N and Skabardonis, A},
journal = {Transp. Res. C},
keywords = {Bayesian networks,Cross-recurrence quantification analysis,Occupancy,Queue spillovers,Short-term traffic flow patterns,Signalized arterials,Traffic flow analysis,Volume},
pages = {392--403},
title = {{Statistical characteristics of transitional queue conditions in signalized arterials}},

volume = {15},
year = {2007}
}
@article{suder1998,
abstract = {This study focuses on the dynamic pattern of heart rate variability in the frequency range of respiration, the so-called respiratory sinus arrhythmia. Forty experimental time series of heart rate data from four healthy adult volunteers undergoing a paced respiration protocol were used as an empirical basis. For pacing-cycle lengths {\textgreater}8 s, the heartbeat intervals are shown to obey a rule that can be expressed by a one-dimensional circle map (next-angle map). Circle maps are introduced as a new type of model for time series analyses to characterize the nonlinear dynamic pattern underlying the respiratory sinus arrhythmia during voluntary paced respiration. Although these maps are not chaotic, the dynamic pattern shows typical imprints of nonlinearity. By starting from a piecewise linear model, which describes the different circle maps obtained from the empirical time series for various pacing frequencies, time invariant measures can be introduced that characterize the dynamic pattern of heart rate variability during voluntary slow-paced respiration.},
annote = {Application},
author = {Suder, K and Drepper, F R and Schiek, M and Abel, H.-H.},
journal = {Am. J. Physiol. -- Hear. Circ. Physiol.},
keywords = {sinus arrhythmia; cardiovascular model; periodicit},
pages = {1092--1099},
title = {{One-dimensional, nonlinear determinism characterizes heart rate pattern during paced respiration}},
volume = {275},
year = {1998}
}
