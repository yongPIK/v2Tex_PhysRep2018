keywords = {Hierarchical organization,Kinematics,Nonlinear dynamics,Spontaneous newborn movements},
pages = {568--586},
title = {{Hierarchical organization of a reference system in newborn spontaneous movements}},

volume = {30},
year = {2007}
}
@article{guillot2000,
abstract = {In previous papers, we have established, through a functional analysis of the behavioral sequences recorded on laboratory mice over a twelve-hour period, the existence of two independent strategies of action selection. On the one hand, the choice of ultradian alternations of rest and activity bouts makes it possible for every mouse to maximize its net energy gain over a one-day or a one-night interval. On the other hand, the succession of acts performed within an activity bout might serve to precisely fit the metabolic needs of each animal, because the corresponding results present great inter-individual variability, with some mice increasing and others decreasing their net energy gain. To determine what kind of dynamic system could generate these successions of acts within an activity bout, we performed a nonlinear time series analysis of the energy costs related to the acts displayed by the mice during such a period. The results suggest that chaotic dynamics might be involved in action selection in mice. Thus, the variability mentioned above could be a consequence of the sensitive dependence on initial conditions associated with such dynamics, and would allow mice to rapidly adapt their metabolic needs to the ongoing situation.},
annote = {Application},
author = {Guillot, A and Meyer, J.-A.},
journal = {Nonlinear Dynamics. Psychol. Life Sci.},
keywords = {action selection,behavioral sequences,chaos,mouse,nonlinear dynamics},
pages = {297--309},
title = {{Chaotic Dynamics Underlying Action Selection in Mice}},

volume = {4},
year = {2000}
}
@article{litak2009b,
abstract = {Vertical vibrations of a delivery car passing through railway tracks have been investigated in this paper. The application of recurrence plots allows to examine short time series of acceleration non-stationary courses. Recurrence quantification analysis and square deviations estimated in small windows have been used to monitor car vibrations and transient behaviour. Measuring acceleration on the "sprung" and "unsprung" masses of a vehicle has enabled also to test the quality of a car suspension.},
