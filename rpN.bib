author = {Novak, V and Schmidt, J},
journal = {Physiol. Res.},
keywords = {Computer simulation of space-temporal summation,RP diagram,Recurrence plot method,Time structures' unification},
pages = {497--505},
title = {{Changes of the Inner Time Structures in the Sequences of Interspike Intervals Produced by the Activity of Excitatory and Inhibitory Synapses: Simulation with Gaussian Input Processes}},
volume = {46},
year = {1997}
}
@article{jensen2007,
abstract = {The segmentation of music into intro-chorus-verse-outro, and similar segments, is a difficult topic. A method for performing automatic segmentation based on features related to rhythm, timbre, and harmony is presented, and compared, between the features and between the features and manual segmentation of a database of 48 songs. Standard information retrieval performance measures are used in the comparison, and it is shown that the timbre-related feature performs best.},
annote = {Related},
author = {Jensen, K},
