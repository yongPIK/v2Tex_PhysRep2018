year = {2005}
}
@article{schauer95,
abstract = {The prevailing theoretical approach to variability and motor control of gait is to treat the within-subject variability as a reflection of noise in the sensorimotor system. Another way to conceptualize noise is to consider the sensorimotor system as a non-linear dynamic system. Here noise is viewed as part of the dynamics and not as superimposed components. Thus noise contributes to the qualitative properties of the dynamic output and not merely to the quantitative properties. This formulation holds the interesting property that noise can be beneficial to the system in the sense of facilitating the adaptation to task demands.With the help of two shoes with eight built-in force sensors and a portable data logger (InfotronicTM) we recorded vertical force peaks during healthy gait during a time interval of 160 s. The data allows assessment of the stationarity of the time series. The time demand to get a stable gait pattern was monitored in dependence on the gait velocity. The stationarity is characterized at first by the recurrence plot, a graphical tool for the diagnosis of drift and hidden periodicities. Further analysis is performed to discriminate between a noisy periodic signal and deterministic chaotic dynamics. The aim of the investigation is to introduce the fractal dimension of the time series of essential gait parameters for the assessment of a practical number of degrees of freedom in the motor control of gait.},
annote = {Application},
author = {Schauer, M and Jahnke, M and Hesse, S and Mauritz, K.-H.},
journal = {Gait Posture},
pages = {174},
title = {{Time series analysis of healthy gait to estimate the sources of variability}},

volume = {3},
year = {1995}
}
@article{acharya2005,
abstract = {Application of non-linear dynamics methods to the physiological sciences demonstrated that non-linear models are useful for understanding complex physiological phenomena such as abrupt transitions and chaotic behavior. Sleep stages and sustained fluctuations of autonomic functions such as temperature, blood pressure, electroencephalogram (EEG), etc., can be described as a chaotic process. The EEG signals are highly subjective and the information about the various states may appear at random in the time scale. Therefore, EEG signal parameters, extracted and analyzed using computers, are highly useful in diagnostics. The sleep data analysis is carried out using non-linear parameters: correlation dimension, fractal dimension, largest Lyapunov entropy, approximate entropy, Hurst exponent, phase space plot and recurrence plots. These non-linear parameters quantify the cortical function at different sleep stages and the results are tabulated.},
annote = {Application},
author = {{Acharya U.}, R and Kannathal, O Faustand N and Chua, T L and Laxminarayan, S},
file = {:Users/yongzou/Documents/RPPapers/NewYear2005{\_}RP/acharya{\_}CompMethProgrBiomed2005.pdf:pdf},
journal = {Comput. Methods Programs Biomed.},
keywords = {Approximate entropy,Correlation dimension,Fractal dimension,Hurst exponent,Lyapunov Exponent,Sleep stages},
pages = {37--45},
title = {{Non-linear analysis of EEG signals at various sleep stages}},

volume = {80},
year = {2005}
}
@article{wendeker2004,
abstract = {We report our results on nonperiodic experimental time series of pressure in a spark ignition engine. The experiments were performed for a low rotational velocity of a crankshaft and a relatively large spark advance angle. We show that the combustion process has many chaotic features. Surprisingly, the reconstructed attractor has a characteristic butterfly shape similar to a chaotic attractor of Lorentz type. The suitable recurrence plot shows that the dynamics of the combustion is a nonlinear multidimensional process mediated by stochastic noise.},
