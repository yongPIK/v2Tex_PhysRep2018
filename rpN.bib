}
@inproceedings{ahlstrom2006c,
abstract = {The 3rd heart sound (S3) is normally heard during auscultation of younger individuals, but it is also common in many patients with heart failure. Compared to the 1st and 2nd heart sounds, S3 has low amplitude and low frequency content, making it hard to detect (both manually for the physician and automatically by a detection algorithm). We present an algorithm based on a recurrence time statistic which is sensitive to changes in a reconstructed state space, particularly for detection of transitions with very low energy. Heart sound signals from ten children were used in this study. Most S3 occurrences were detected (98{\%}), but the amount of false extra detections was rather high (7{\%} of the heart cycles). In conclusion, the method seems capable of detecting S3 with high accuracy and robustness.},
annote = {Application},
author = {Ahlstrom, C and Hult, P and Ask, P},
booktitle = {Proc. {\{}IEEE{\}} Conf. Acoust. Speech Signal Process. (ICASSP 2006)},
pages = {II1040----II1043},
title = {{Detection of the 3rd heart sound using recurrence time statistics}},

volume = {2},
year = {2006}
}
@article{paulus2009,
abstract = {This paper proposes a method for recovering the sectional form of a musical piece from an acoustic signal. The description of form consists of a segmentation of the piece into musical parts, grouping of the segments representing the same part, and assigning musically meaningful labels, such as ldquochorusrdquo or ldquoverse,rdquo to the groups. The method uses a fitness function for the descriptions to select the one with the highest match with the acoustic properties of the input piece. Different aspects of the input signal are described with three acoustic features: mel-frequency cepstral coefficients, chroma, and rhythmogram. The features are used to estimate the probability that two segments in the description are repeats of each other, and the probabilities are used to determine the total fitness of the description. Creating the candidate descriptions is a combinatorial problem and a novel greedy algorithm constructing descriptions gradually is proposed to solve it. The group labeling utilizes a musicological model consisting of N-grams. The proposed method is evaluated on three data sets of musical pieces with manually annotated ground truth. The evaluations show that the proposed method is able to recover the structural description more accurately than the state-of-the-art reference method.},
annote = {Application},
author = {Paulus, J and Klapuri, A},
journal = {IEEE Trans. Audio. Speech. Lang. Processing},
pages = {1159--1170},
title = {{Music Structure Analysis Using a Probabilistic Fitness Measure and a Greedy Search Algorithm}},

volume = {17},
year = {2009}
}
@article{robinson2009,
abstract = {We show that under suitable assumptions, Poincar{\&}eacute; recurrences of a dynamical system determine its topology in phase space. Therefore, dynamical systems with the same recurrences are dynamically equivalent. This conclusion can be drawn from a theorem proved in this paper which states that the recurrence matrix determines the topology of closed sets. The theorem states that if a set of points {\textless}i{\textgreater}M{\textless}/i{\textgreater} is mapped onto another set {\textless}i{\textgreater}N{\textless}/i{\textgreater}, such that two points in {\textless}i{\textgreater}N{\textless}/i{\textgreater} are closer than some prescribed fixed distance if and only if the corresponding points in {\textless}i{\textgreater}M{\textless}/i{\textgreater} are closer than some, in general different, prescribed fixed distance, then both sets are homeomorphic, i.e., identical up to a continuous change in the coordinate system. The theorem justifies a range of methods in nonlinear dynamics which are based on recurrence properties.},
annote = {Basics},
author = {Robinson, G and Thiel, M},
file = {:Users/yongzou/Documents/Climate{\_}Network2008Papers/RecurrencePlotPIK/Recurrences determine the dynamics.pdf:pdf},
journal = {Chaos},
pages = {23104},
title = {{Recurrences determine the dynamics}},

volume = {19},
year = {2009}
}
@article{dasan2002,
abstract = {We report an analysis, using the tools of nonlinear dynamics and chaos theory, of the fluctuations in the stress determined from simulations of shear flow of Stokesian suspensions. The simulations are for shear between plane parallel walls of a suspension of rigid identical spheres in a Newtonian fluid, over a range of particle concentration. By analyzing the time series of the stress, we find that the dynamics underlying these fluctuations is deterministic, low-dimensional, and chaotic. We use the dynamic and metric invariants of the underlying dynamics as a means of characterizing suspension behavior. The dimension of the chaotic attractor increases with particle concentration, indicating the increasing influence of multiple-body interactions on the rheology of the suspension with rise in particle concentration. We use our analysis to make accurate predictions of the short-term evolution of a stress component from its preceding time series, and predict the evolution of one component of the stress using the time series of another. We comment on the physical origin of the chaotic stress fluctuations, and on the implications of our results on the relation between the microstructure and the stress.},
annote = {Application},
author = {Dasan, J and Ramamohan, T R},
journal = {Phys. Rev. E},
pages = {21409},
title = {{Stress fluctuations in sheared Stokesian suspensions}},

volume = {66},
year = {2002}
}
@article{deVera2008,
abstract = {Both nonlinear and fractal properties of beat-to-beat R-R interval variability signal (RRV) of freely moving lizards ({\textless}i{\textgreater}Gallotia galloti{\textless}/i{\textgreater}) were studied in baseline and under autonomic nervous system blockade. Nonlinear techniques allowed us to study the complexity, chaotic behavior, nonlinearity, stationarity, and regularity over time of RRV. Scaling behavior of RRV was studied by means of fractal techniques. The autonomic nervous system blockers used were atropine, propranolol, prazosin, and yohimbine. The nature of RRV was linear in baseline and under {\&}beta;-, {\&}alpha;{\textless}sub{\textgreater}1{\textless}/sub{\textgreater}- and {\&}alpha;{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}-adrenoceptor blockades. Atropine changed the linear nature of RRV to nonlinear and increased its stationarity, regularity and fractality. Propranolol increased the complexity and chaotic behavior, and decreased the stationarity, regularity, and fractality of RRV. Both prazosin and yohimbine did not change any of the nonlinear and fractal properties of RRV. It is suggested that {\textless}i{\textgreater}1{\textless}/i{\textgreater}) the use of both nonlinear and fractal analysis is an appropriate approach for studying cardiac period variability in reptiles; {\textless}i{\textgreater}2{\textless}/i{\textgreater}) the cholinergic activity, which seems to make the {\&}alpha;{\textless}sub{\textgreater}1{\textless}/sub{\textgreater}-, {\&}alpha;{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}- and {\&}beta;-adrenergic activity interaction unnecessary, determines the linear behavior in basal RRV; {\textless}i{\textgreater}3{\textless}/i{\textgreater}) fractality, as well as both RRV regularity and stationarity over time, may result from the balance between cholinergic and {\&}beta;-adrenergic activities opposing actions; {\textless}i{\textgreater}4{\textless}/i{\textgreater}) {\&}beta;-adrenergic activity may buffer both the complexity and chaotic behavior of RRV, and {\textless}i{\textgreater}5{\textless}/i{\textgreater}) neither the {\&}alpha;{\textless}sub{\textgreater}1{\textless}/sub{\textgreater}- nor the {\&}alpha;{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}-adrenergic activity seem to be involved in the mediation of either nonlinear or fractal components of RRV.},
annote = {Application},
author = {{De Vera}, L and Santana, A and Gonzalez, J J},
journal = {Am. J. Physiol.},
keywords = {autonomic nervous system,fractal analysis,nonlinear analysis,recurrence plot,telemetry},
pages = {R1282----R1289},
title = {{Nonlinearity and fractality in the variability of cardiac period in the lizard, Gallotia galloti: effects of autonomic blockade}},

volume = {295},
year = {2008}
}
@inproceedings{yanli2005,
abstract = {To test whether electromyogram (EMG) is nonlinear deterministic signal or just random noise, we analyzed four EMGs data of an adult woman. EMG is assessed with the correlation dimension (D{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}), maximum Lyapunov exponent ({\&}lambda;{\textless}sub{\textgreater}1{\textless}/sub{\textgreater}), approximate entropy(ApEn), recurrence plot analysis (RPA) and recurrence quantification analysis (RQA). From all the results of these assessments we can conclude that EMG obeys a certain nonlinear deterministic law and nonstationarity is significant within this signal.},
annote = {Application},
author = {Yanli, M and Yuping, L and Bingzheng, L},
