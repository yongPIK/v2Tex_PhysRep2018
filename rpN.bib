title = {{Blind Extraction of Chaotic Sources from White Gaussian Noise Based on a Measure of Determinism}},

volume = {5441},
year = {2009}
}
@article{yu1998,
abstract = {We propose a simple method to efficiently probe dynamical nonstationarity in observed time series. In a time-index plot, the density distributions as a function of normalized time-index are V-shaped due to nonstationarity. We show that this method is workable for short data sets and typical examples are illustrated.},
