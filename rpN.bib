author = {Lathrop, D P and Kostelich, E J},
journal = {Phys. Rev. A},
pages = {4028--4031},
title = {{Characterization of an experimental strange attractor by periodic orbits}},

volume = {40},
year = {1989}
}
@misc{marwan99,
abstract = {Nonlinear time series analysis is used to compare the impact of climate oscillations in the Pacific (El Ni{\&}ntilde;o/Southern Oscillation) and in the Atlantic (Tropical Atlantic Dipole) on present and past rainfall variations in NW Argentina. Past rainfall variations have been reconstructed from 30,000 years old varved lake sediments. In the analysis the methods of Recurrence Plot, Cross Recurrence Plot and Recurrence Quantification Analysis are applied. Similarities between the dynamics of climate oscillations and recent and past precipitation are detected. In addition these very new methods are studied by application to known examples and models.},
address = {Dresden University of Technology},
