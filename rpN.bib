}
@article{litak2008b,
abstract = {We investigate the time series of a torque applied to the ripping head in the process of a cutting concrete rock with sharp and blunt tools. By applying nonlinear embedding methods and the recurrence plots technique to the corresponding time series, we indicate the changes in nonlinear dynamics lying behind the ripping process, and we propose a test of the ripping machine efficiency and a way of monitoring the state of tools.},
