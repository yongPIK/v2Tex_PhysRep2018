year = {2009}
}
@article{michalowicz2008,
abstract = {This work considers the problem of detecting signals in noise in the absence of a well-defined signal model. Specifically, we compare detectors based on recurrence plots to one of the more commonly used detection strategies. Results indicate improvements are possible using the recurrence-based detectors for certain signal-to-noise ratios.},
