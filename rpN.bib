file = {::},
journal = {J. Syst. Integr.},
keywords = {approximate entropy,central tendency measure,dynamical systems,heart rate variability,lyapunov exponents,nonlinear measures,nonlinear prediction,recurrence plot,stationarity},
pages = {41--53},
title = {{Validity Test for a Set of Nonlinear Measures for Short Data Length with Reference to Short-Term Heart Rate Variability Signal}},
