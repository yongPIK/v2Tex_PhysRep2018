annote = {Application},
author = {{De Vera}, L and Santana, A and Gonzalez, J J},
journal = {Am. J. Physiol. -- Regul. Integr. Comp. Physiol.},
keywords = {autonomic nervous system,fractal analysis,nonlinear analysis,recurrence plot,telemetry},
pages = {R1282----R1289},
title = {{Nonlinearity and fractality in the variability of cardiac period in the lizard, Gallotia galloti: effects of autonomic blockade}},

