year = {2004}
}
@article{zbilut2000a,
abstract = {A simple data analysis strategy based on the scoring of recurrences, allowed us to distinguish relatively short runs of (i) a chaotic system (logistic map), (ii) transcendental numbers (small pi, Greek), (iii) pseudo-random numbers, and (iv) physical random numbers (atmospheric noise). Beside the theoretical implications of distinguishing transcendental and pseudo-random numbers from physical noise, these results suggest a test},
annote = {Basics},
author = {Zbilut, J P and iani, A and {L. Webber Jr.}, C},
file = {:Users/yongzou/Documents/RPPapers/zbilut{\_}PLA{\_}2000.pdf:pdf},
